      <!--field name='out_format' eval="ref('report_aeroo.report_mimetypes_ods_ods')"/-->
      <field name='multi' eval='False'/>
      <field name='name'>PCI Financial Report</field>
      <field name='report_rml'>account_financial_report_comparison/report/bs_sequential_month.ods</field>
      <field name='report_name'>bs_sequential_month_report</field>
      <field name='model'>account.move</field>
      <field name='type'>ir.actions.report.xml</field>
      <field name='attachment_use' eval='False'/>
      <field name='parser_loc'>account_financial_report_comparison/report/bs_sequential_month.py</field>
      <field name='tml_source'>file</field>
      <field name='parser_state'>loc</field>
      <field name='styles_mode'>default</field>
